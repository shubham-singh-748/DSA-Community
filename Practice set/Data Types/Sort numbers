#include <bits/stdc++.h>
using namespace std;

void find_asc(int a, int b, int c)
{
	int get_max = max(a, max(b, c));
	int get_min = min(a, min(b, c));
	int get_mid = (a+b+c) - (get_max+get_min);

	cout << get_min << endl;
	cout << get_mid << endl;
	cout << get_max << endl;
}

void print_as(int a, int b, int c)
{
	cout << a << endl;
	cout << b << endl;
	cout << c << endl;
}
int main(int argc, char const *argv[])
{
	long long int a, b, c;
	cin >> a >> b >> c;

	find_asc(a, b, c);
	cout << endl;
	print_as(a, b, c);

}
