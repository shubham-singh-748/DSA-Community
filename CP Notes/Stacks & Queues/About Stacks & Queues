#include <bits/stdc++.h>
using namespace std;

int main(int argc, char const *argv[])
{
	// About Stacks
	stack<int> s;
	s.push(10);
	s.push(20);
	s.push(30);
	s.push(40);

	while(!s.empty())
	{
		cout << s.top() << endl;
		s.pop();
	}

	// About queue
	queue<string> q;
	q.push("abc");
	q.push("def");
	q.push("ghi");
	q.push("jkl");
	q.push("mno");

	cout << endl;

	while(!q.empty())
	{
		cout << q.front() << endl;
		q.pop();
	}
	return 0;
}
