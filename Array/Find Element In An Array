//Write a program to represent index at which d is found in an
//array a and -1 if not found.

import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) throws Exception {
    // write your code here
    Scanner scn = new Scanner(System.in);

    int n = scn.nextInt();
    int[] arr = new int[n];
    for(int i=0; i<arr.length; i++)
    {
        arr[i] = scn.nextInt();
    }

    int data = scn.nextInt();
    int Ind = -1;
    for(int i=0; i<arr.length; i++)
    {
        if(arr[i] == data)
        {
            Ind = i;
            break;
        }
    }
    System.out.println(Ind);
  }
}
