#include <bits/stdc++.h>
using namespace std;

void CountDistinct(string s)
{
	unordered_set<char> c;
	for(int i=0; i<s.length(); ++i)
	{
		c.insert(s[i]);
	}

	if(c.size()%2 == 0) cout << "CHAT WITH HER!\n";
	else cout << "IGNORE HIM!\n";
}
int main(int argc, char const *argv[])
{
	string s; cin >> s;
	CountDistinct(s);

	return 0;
}
