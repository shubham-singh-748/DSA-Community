    #include <bits/stdc++.h>
    using namespace std;
     
    int main(int argc, char const *argv[])
    {
        string ans = "";
     
        string s1, s2; cin >> s1 >> s2;
        int length = s1.length();
     
        for(int i=0; i<length; ++i)
        {
            if(s1[i] == s2[i])
            {
                ans += '0';
            }
            else
            {
                ans += '1';
            }
        }
        cout << ans;
        return 0;
    }
